{
    "title": "Cassandra",
    "columns": 3,
    "user": "erez",
    "timeBack": "1h",
    "from": "",
    "until": "",
    "width": 350,
    "height": 350,
    "legend": true,
    "refresh": true,
    "refreshIntervalSeconds": 30,
    "averageSeries": false,
    "defaultLineWidth": 1,
    "tz":"America/Sao_Paulo",
    "data": [
        {
            "title": "Load",
            "target": "aliasByNode(maximumAbove(application.thoth.aca-*.cassandra_load,1),2)"
        },
        {
            "title": "Read Latency",
            "target": "aliasByNode(maximumAbove(application.thoth.aca-*.read_latency,1),2)"
        },
        {
            "title": "Write Latency",
            "target": "aliasByNode(maximumAbove(application.thoth.aca-*.write_latency,1),2)"
        },
        {
            "title": "Range Slice Latency",
            "target": "aliasByNode(maximumAbove(application.thoth.aca-*.range_slice_latency,1),2)"
        },
        {
            "title": "Used Heap (moving average)",
            "target": "aliasByNode(scale(movingAverage(maximumAbove(application.thoth.aca-*.used_heap,1),'5min'),0.000000000233),2)"
        },
        {
            "title": "Key cache hits",
            "target": "aliasByNode(maximumAbove(application.thoth.aca-*.keycache_hits,1),2)"
        },
        {
            "title": "Key cache requests",
            "target": "aliasByNode(maximumAbove(application.thoth.aca-*.keycache_requests,1),2)"
        },
        {
            "title": "Key cache size",
            "target": "aliasByNode(maximumAbove(application.thoth.aca-*.keycache_size,1),2)"
        },
        {
            "title": "Hit Rate",
            "target": ["aliasByNode(divideSeries(application.thoth.aca-1.keycache_hits,application.thoth.aca-1.keycache_requests),2)",
			"aliasByNode(divideSeries(application.thoth.aca-2.keycache_hits,application.thoth.aca-2.keycache_requests),2)",
			"aliasByNode(divideSeries(application.thoth.aca-3.keycache_hits,application.thoth.aca-3.keycache_requests),2)",
			"aliasByNode(divideSeries(application.thoth.aca-4.keycache_hits,application.thoth.aca-4.keycache_requests),2)",
			"aliasByNode(divideSeries(application.thoth.aca-5.keycache_hits,application.thoth.aca-5.keycache_requests),2)",
			"aliasByNode(divideSeries(application.thoth.aca-6.keycache_hits,application.thoth.aca-6.keycache_requests),2)",
			"aliasByNode(divideSeries(application.thoth.aca-7.keycache_hits,application.thoth.aca-7.keycache_requests),2)",
			"aliasByNode(divideSeries(application.thoth.aca-8.keycache_hits,application.thoth.aca-8.keycache_requests),2)",
			"aliasByNode(divideSeries(application.thoth.aca-9.keycache_hits,application.thoth.aca-9.keycache_requests),2)"]
        },
        {
            "title": "Last Parnew GC Duration",
            "target": "aliasByNode(maximumAbove(application.thoth.aca-*.last_parnew_gc_duration,1),2)"
        },
        {
            "title": "Last Concurrent MS GC Duration",
            "target": "aliasByNode(maximumAbove(application.thoth.aca-*.last_concurrentms_gc_duration,1),2)"
        },
        {
            "title": "Network RX",
            "target": "aliasByNode(collectd.aca*.interface-eth0.if_packets.rx,1)"
        },
        {
            "title": "Network TX",
            "target": "aliasByNode(collectd.aca*.interface-eth0.if_packets.tx,1)"
        }
    ],
    "parameters": {
        "datacenter": {
            "All": {
                "all": "*"
            }
        }

    }
}

